<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.syed.musicstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label= "@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatePlaylist"
            android:label="Speed Music" />
        <activity android:name=".Playlist"
            android:label="Playlist"/>
        <activity
            android:name=".Genres"
            android:label="Genres" />
        <activity
            android:name=".TopCharts"
            android:label="Top Charts" />
        <activity
            android:name=".Login"
            android:label="Login Settings"></activity>
        <activity android:name=".NowPlaying"
            android:label="Now Playing"></activity>
    </application>

</manifest>